<?xml version="1.0" encoding="UTF-8"?>
<feature
      id="com.lembed.codeanalyzer.ui.feature"
      label="%featureName"
      version="0.1.1.qualifier"
      provider-name="%featureProvider"
      license-feature="org.eclipse.license"
      license-feature-version="0.0.0">

   <description url="http://www.example.com/description">
      %featureDescription
   </description>

   <copyright url="http://www.example.com/copyright">
      %copyright
   </copyright>

   <license url="%licenseURL">
      %license
   </license>

   <requires>
      <import plugin="org.eclipse.core.runtime"/>
      <import plugin="org.eclipse.core.resources"/>
      <import plugin="org.eclipse.ui"/>
      <import plugin="org.eclipse.ui.ide"/>
      <import plugin="org.eclipse.jface.text"/>
      <import plugin="org.eclipse.ui.workbench.texteditor"/>
      <import plugin="org.eclipse.core.filesystem"/>
      <import plugin="org.eclipse.ui.editors"/>
      <import plugin="org.eclipse.cdt.ui"/>
      <import plugin="org.eclipse.cdt.core"/>
      <import plugin="org.eclipse.core.resources" version="3.4.2" match="greaterOrEqual"/>
      <import plugin="com.ibm.icu"/>
      <import plugin="org.swtchart"/>
   </requires>

   <plugin
         id="com.lembed.codeanalyzer.ui"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

   <plugin
         id="com.lembed.codeanalyzer.ui.charts"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

   <plugin
         id="com.lembed.codeanalyzer.ui.piechart"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

</feature>
